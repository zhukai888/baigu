.navTab {
  position: fixed;
  top: 0;
  left: 0;
  height: 70rpx;
  width: 100%;
  white-space: nowrap;
  z-index: 99999;
  background-color: #fff;
}

.navTab .scroll {
  display: flex;
}

.navTab .nav {
  padding: 0 30rpx;
  text-align: center;
  display: flex;
  flex-direction: column;
  color: #666;
  font-size: 30rpx;
}

.navTab .active>text {
  font-size: 32rpx;
  font-weight: bold;
  color: rgba(26, 26, 26, 1);
}

.navTab image {
  width: 36rpx;
  height: 13rpx;
  margin: 0 auto;
  margin-top: 10rpx;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.container {
  padding-top: 70rpx;
  min-height: 100%;
  /* height: 100%; */
  box-sizing: border-box;
  background-color: #f7f7f7;
}

.noData {
  padding-top: 200rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 40rpx;
}

.goodsList {
  /* background-color: #f7f7f7; */
  padding: 30rpx 24rpx;
  border-radius: 26rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  min-height: 100%;
  align-items: flex-start;
  box-sizing: border-box;
}

.goodsList .good {
  /* height: 500rpx; */
  position: relative;
  border-radius: 6rpx;
  font-size: 0;
  /* overflow: hidden; */
  margin-bottom: 20rpx;
}

.goodsList .good>image {
  width: 340rpx;
  height: 340rpx;
}

.goodsList .good .g-content {
  font-size: 28rpx;
  color: #1b1b1b;
  padding: 20rpx;
  background-color: #fff;
  box-sizing: border-box;
  width: 340rpx;
}

.goodsList .good .g-content .g-price {
  padding-top: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.goodsList .good .g-content .g-price>view {
  font-size: 24rpx;
}

.goodsList .good .g-content .g-price>view>text:nth-child(1) {
  font-size: 32rpx;
  color: #f00;
}

.goodsList .good .g-content .g-price>view>text:nth-child(2) {
  padding-left: 20rpx;
  color: #999;
}

.goodsList .good .g-content .g-price>image {
  width: 30rpx;
  height: 6rpx;
  padding: 20rpx 0rpx 20rpx 20rpx;
}

.goodsList .good .shadow {
  position: absolute;
  top: 0;
  left: 0;
  /* height: 473rpx; */
  width: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  display: flex;
  flex-direction: column;
  z-index: 99;
}

.goodsList .good .shadow .s-header {
  min-height: 340rpx;
  padding: 46rpx 20rpx 0;
  box-sizing: border-box;
}

.goodsList .good .shadow .s-header>view, .goodsList .good .shadow .s-header>button{
  height: 70rpx;
  width: 300rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 35rpx;
  margin-bottom: 20rpx;
  font-size: 26rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight: 400;
  padding:0;
}

.goodsList .good .shadow .s-header>view>image,.goodsList .good .shadow .s-header>button>image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 24rpx;
}

.goodsList .good .shadow .s-bottom {
  flex: 1;
  height: 160rpx;
  background-color: #000;
  display: flex;
  justify-content: center;
  align-items: center;
}

.goodsList .good .shadow .s-bottom>image {
  width: 36rpx;
  height: 25rpx;
  padding: 30rpx;
}

.car {
  position: fixed;
  right: 0rpx;
  bottom: 20rpx;
  z-index: 199;
}

.car>image {
  width: 154rpx;
  height: 154rpx;
}

.carShadow {
  position: fixed;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  z-index: 150;
  background-color: rgba(0, 0, 0, 0.5);
  /* padding: 130rpx 62rpx 0; */
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
}

.carDetail {
  background-color: #fff;
  box-sizing: border-box;
  padding: 30rpx;
  border-radius: 12rpx;
  width: 700rpx;
  height: 1000rpx;
}

.carDetail .header {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.carDetail .header .left {
  font-size: 38rpx;
  font-weight: bold;
  color: rgba(24, 24, 24, 1);
}

.carDetail .header .left>image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 16rpx;
}

.carDetail .header .right {
  font-size: 26rpx;
  color: #191919;
  display: flex;
  align-items: center;
}

.carDetail .header .right>image {
  width: 40rpx;
  height: 28rpx;
  margin-right: 10rpx;
}

.carDetail .header .right>view {
  position: relative;
  width: 210rpx;
  height: 34rpx;
  line-height: 34rpx;
  background: rgba(242, 242, 242, 1);
  border-radius: 17rpx;
  text-align: center;
}

.carDetail .header .right>view>view:nth-child(2) {
  margin-top: 2rpx;
  position: absolute;
  width: 210rpx;
  line-height: 34rpx;
  background: rgba(242, 242, 242, 1);
  border-radius: 8rpx;
  text-align: center;
  padding-bottom: 20rpx;
}

.carDetail .header .right>view>view:nth-child(2)>view {
  padding-top: 20rpx;
}

.carDetail .content {
  padding-top: 20rpx;
  height: 670rpx;
  overflow: scroll;
  box-sizing: border-box;
  -webkit-overflow-scrolling: touch;
}

.content .cargood {
  display: flex;
  align-items: center;
  margin-top: 20rpx;
}

.content .cargood>image:nth-child(1) {
  max-width: 20rpx;
  height: 20rpx;
  min-width: 20rpx;
}

.content .cargood>image:nth-child(2) {
  min-width: 130rpx;
  max-width: 130rpx;
  height: 130rpx;
  border-radius: 6rpx;
  margin: 0 18rpx 0 20rpx;
}

.content .cargood>view {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 130rpx;
  padding: 10rpx 0;
  box-sizing: border-box;
}

.content .cargood .cg-title, .content .cargood .cg-price {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.content .cargood .cg-title {
  font-size: 32rpx;
  color: #1b1b1b;
}

.content .cargood .cg-title> .omit {
  max-width: 300rpx;
}

.content .cargood .cg-title>image {
  width: 28rpx;
  height: 28rpx;
}

.content .cargood .cg-price {
  font-size: 32rpx;
}

.content .cargood .cg-price>view:nth-child(2) {
  border: 2rpx solid #e6e6e6;
  display: flex;
  height: 40rpx;
}

.content .cargood .cg-price>view:nth-child(2) view {
  height: 40rpx;
  line-height: 40rpx;
  color: #999;
  padding: 0 4rpx;
  width: 40rpx;
  text-align: center;
}

.content .cargood .cg-price>view:nth-child(2) input {
  width: 28px;
  border-left: 1px solid #e6e6e6;
  border-right: 1px solid #e6e6e6;
  max-height: 40rpx;
  min-height: 40rpx;
  font-size: 24rpx;
  color: #999;
  display: flex;
  align-items: flex-start;
  justify-content: center;
  text-align: center;
}

.total {
  font-size: 32rpx;
  color: #1b1b1b;
  text-align: center;
  padding: 20rpx 0 15rpx;
  border-top: 7rpx solid #f2f2f2;
}

.pay {
  height: 110rpx;
  background: rgba(255, 214, 24, 1);
  border-radius: 8rpx;
  font-size: 32rpx;
  color: rgba(27, 27, 27, 1);
  text-align: center;
  line-height: 110rpx;
}

.goodShadow {
  position: fixed;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  z-index: 200;
  background-color: rgba(0, 0, 0, 0.5);
  padding: 30rpx 24rpx 0;
  box-sizing: border-box;
  overflow: scroll;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.gs-detail {
  padding: 20rpx 20rpx 30rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-radius: 12rpx;
  width: 700rpx;
}

.gs-detail swiper {
  height: 660rpx;
  width: 100%;
  margin-bottom: 20rpx;
}

.gs-detail swiper image {
  width: 100%;
  height: 660rpx;
  border-radius: 12rpx;
}

.gs-detail>.omit {
  font-size: 38rpx;
  color: #1b1b1b;
}

.gs-detail .gs-price {
  font-size: 34rpx;
  display: flex;
  justify-content: space-between;
  padding-top: 20rpx;
}

.gs-detail .gs-price>view:nth-child(2) {
  border: 2rpx solid #e6e6e6;
  display: flex;
  height: 52rpx;
}

.gs-detail .gs-price>view:nth-child(2) view {
  height: 52rpx;
  line-height: 52rpx;
  color: #999;
  padding: 0 10rpx;
}

.gs-detail .gs-price>view:nth-child(2) input {
  width: 100rpx;
  border-left: 1rpx solid #e6e6e6;
  border-right: 1rpx solid #e6e6e6;
  height: 52rpx;
  font-size: 32rpx;
  color: #999;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}

.option {
  padding-top: 30rpx;
  display: flex;
  justify-content: space-between;
  line-height: 110rpx;
  font-size: 32rpx;
  color: rgba(27, 27, 27, 1);
}

.option view:nth-child(1) {
  width: 226rpx;
  height: 110rpx;
  background: rgba(255, 214, 24, 1);
  border-radius: 8rpx;
  text-align: center;
}

.option view:nth-child(2) {
  width: 426rpx;
  height: 110rpx;
  background: rgba(255, 214, 24, 1);
  border-radius: 8rpx;
  text-align: center;
}

.close {
  padding-top: 30rpx;
  display: flex;
  justify-content: center;
}

.close image {
  width: 80rpx;
  height: 80rpx;
}
